 [Unit]
 Description=TravelFrog stub backend
 After=network.target
 
 [Service]
 Type=simple
 WorkingDirectory=/usr/share/nginx/html/apps/TravelFrog/main/stubs/api
 ExecStart=/usr/bin/node server.js
 Restart=on-failure
 Environment=NODE_ENV=production PORT=5000
 
 [Install]
 WantedBy=multi-user.target
